{
  "name": "react-server-renderer",
  "version": "1.1.1",
  "description": "simple React SSR solution inspired by vue-server-render",
  "repository": "git@github.com:JounQin/react-server-renderer.git",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "scripts": {
    "lint": "tslint -p . -t stylish",
    "build": "tsc"
  },
  "files": [
    "lib",
    "client-plugin.js",
    "client-plugin.d.ts",
    "server-plugin.js",
    "server-plugin.d.ts"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "tsc"
    }
  },
  "lint-staged": {
    "*.ts": [
      "tslint -p . -t stylish --fix",
      "git add"
    ],
    "*.{md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "peerDependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "dependencies": {
    "hash-sum": "^1.0.2",
    "lodash": "^4.17.11",
    "lodash.uniq": "^4.5.0",
    "resolve": "^1.8.1",
    "serialize-javascript": "^1.5.0",
    "source-map": "^0.7.0"
  },
  "devDependencies": {
    "@types/hash-sum": "^1.0.0",
    "@types/lodash.uniq": "^4.5.4",
    "@types/node": "^10.12.0",
    "@types/react": "^16.7.4",
    "@types/react-dom": "^16.0.9",
    "@types/resolve": "^0.0.8",
    "@types/serialize-javascript": "^1.5.0",
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-plugin-prettier": "^2.0.0",
    "typescript": "^3.1.3"
  }
}
